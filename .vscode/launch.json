{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "dist/index.js",
            "skipFiles": ["<node_internals>/**"],
            "env": {
                "NODE_ENV": "development"
            },
            "program": "${workspaceFolder}/dist/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "mdcode merge",
            "skipFiles": ["<node_internals>/**"],
            "env": {
                "NODE_ENV": "development"
            },
            "args": ["merge", "debug/index.md"],
            "program": "${workspaceFolder}/dist/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "mdcode extract",
            "skipFiles": ["<node_internals>/**"],
            "env": {
                "NODE_ENV": "development"
            },
            "args": ["extract", "debug/index.md"],
            "program": "${workspaceFolder}/dist/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "mdcode append",
            "skipFiles": ["<node_internals>/**"],
            "env": {
                "NODE_ENV": "development"
            },
            "args": ["append", "debug/index.md"],
            "program": "${workspaceFolder}/dist/index.js"
        }
    ]
}
